<xs:schema id="Root" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Root">
    <xs:complexType>
      <xs:sequence minOccurs="1" maxOccurs="1">
        <xs:element name="Config">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Files">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ICTestBinPath">
                      <xs:complexType>
                        <xs:attribute name="path" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
